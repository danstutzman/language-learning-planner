{
  "compilerOptions": {
    "noImplicitAny": true,
    "outDir": "./dist/",
    "module": "es6",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "lib": ["DOM", "ScriptHost", "es2015.promise", "es2017"]
  }
}
